{
  "manifest_version": 3,
  "name": "Bookmark",
  "version": "1",
  "permissions": [
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Click to open panel",
    "default_icon": {
      "16": "icons/bookmark16px.png",
      "32": "icons/bookmark32px.png",
      "48": "icons/bookmark48px.png",
      "128": "icons/bookmark128px.png"
    }
  },
  "icons": {
    "16": "icons/bookmark16px.png",
    "32": "icons/bookmark32px.png",
    "48": "icons/bookmark48px.png",
    "128": "icons/bookmark128px.png"
  }
}